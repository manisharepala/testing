.container{style:'margin-top:4%;'}
  %h3 Upload Zip to create Questions from ETX's
  %br
  %div
    =form_tag(assessment_post_zip_upload_only_questions_path,{:multipart=>true,:method=>:post}) do
      =label_tag :file,"Choose Publisher"
      = select_tag "publisher_question_bank_id", options_for_select(@publisher_question_banks.map {|pqb| [pqb.name, pqb.id] }.reverse), class:'btn btn-default'
      %br
      %br
      =file_field_tag :zip_file, accept: 'zip', class:'btn btn-default'
      %br
      %br
      =submit_tag "Upload", class:'btn btn-default'

















  